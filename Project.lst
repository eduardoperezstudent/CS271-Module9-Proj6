				mGetString MACRO str_Message, buffer, bufferSize, fileByteSize
				    PUSH    EAX
				    PUSH    ECX
				    PUSH    EDX
				    ; Display prompt message
				    MOV EDX, OFFSET str_Message
				    CALL WriteString  
				    
				    ; Get user input
				    MOV EDX, OFFSET buffer
				    MOV ECX, bufferSize
				    CALL ReadString  

				    ; Store number of bytes read
				    MOV fileByteSize, EAX

				    POP     EDX
				    POP     ECX
				    POP     EAX
				ENDM

